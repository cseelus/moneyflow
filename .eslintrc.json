{
  "extends": ["next/core-web-vitals", "eslint-config-prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "prefer-const": "error",
    "no-var": "error",
    "no-console": "warn",
    "eqeqeq": "error",
    "curly": "error"
  },
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  }
}
